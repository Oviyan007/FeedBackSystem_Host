{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "feedbackSystem.settings",
    "SECRET_KEY": "@vercel/secure-key"
  },
  "buildCommand": "python manage.py collectstatic --noinput",
  "postinstall": "python manage.py migrate && python manage.py collectstatic --noinput"
}